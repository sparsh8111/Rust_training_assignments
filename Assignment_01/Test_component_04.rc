fn get_day_type(day: u8) -> &'static str {
    match day {
        1 => "Monday",
        2 => "Tuesday",
        3 => "Wednesday",
        4 => "Thursday",
        5 => "Friday",
        6 => "Saturday",
        7 => "Sunday",
        _ => "Invalid day number",
    }
}

fn main() {
    let day_number = 3;
    println!("Day type: {}", get_day_type(day_number));
}
